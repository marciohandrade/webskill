        // Smooth scroll for anchor links - MANTIDO
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // MUDAN√áA 12: Countdown real funcionando
        function updateCountdown() {
            const deadline = new Date('2025-02-15 23:59:59');
            const now = new Date();
            const diff = deadline - now;
            
            if (diff > 0) {
                const days = Math.floor(diff / (1000 * 60 * 60 * 24));
                const hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                document.getElementById('countdown').innerHTML = `${days}d ${hours}h`;
            } else {
                document.getElementById('countdown').innerHTML = 'OFERTA ENCERRADA';
            }
        }
        
        // MUDAN√áA 13: Varia√ß√£o din√¢mica de vagas
        function updateVagas() {
            const vagas = Math.floor(Math.random() * 4) + 3; // Entre 3-6
            document.getElementById('vagas-restantes').innerHTML = vagas;
        }

        // Form submission - OTIMIZADO
        document.getElementById('leadForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            // Capturar todos os dados
