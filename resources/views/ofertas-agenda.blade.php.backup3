            this.reset();
        });

        // Inicializar fun√ß√µes
        updateCountdown();
        updateVagas();
        setInterval(updateCountdown, 60000); // Atualizar countdown a cada minuto
        setInterval(updateVagas, 180000); // Atualizar vagas a cada 3 minutos
    </script>
